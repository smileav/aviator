            </svg>
            {{ text_all_orders }}</a>
        </div>
        <div class="list_orders">
            {% if orders %}
            {% for order in orders %}
              <a href="{{ order.view }}" class="order">
                <div class="order_info">
                  <div class="head">
                    <div class="order_id">{{ text_my_orders }}: {{ order.order_id }}</div>
                    <div class="count">{{ order.products_count }} {{ text_product }}</div>
                  </div>
                  <div class="wrap_totals">
                    <div class="totals">
                      <div class="total">{{ order.total }}</div>
                      <div class="date">{{ order.date_added }}</div>
                    </div>
                    <div class="status" style="background:#{{ order.status_bg_color }};color: #{{ order.status_color }}">{{ order.status }}</div>
                  </div>
                </div>
                {% if order.products %}
                  <div class="products">
                    {% for product in order.products %}
                      <div class="product">
                        <img src="{{ product.image }}" />
                      </div>
                    {% endfor %}
                  </div>
                {% endif %}
              </a>
            {% endfor %}
          {% else %}
            <div class="orders_empty">
