{{ header }}
<style>
  #information-contact h1 {
    font-size: 32px;
    line-height: 100%;
    margin: 0 0 32px;
  }
  #information-contact h4 {
    font-size: 32px;
    line-height: 100%;
    margin: 0 0 32px;
  }
  .contact-inner {
    background-color: #f1f2f5;
    padding: 32px;
    margin-bottom: 24px;
    max-width: 1040px;
  }
  .contact-inner .caret {
    display: none;
  }
  .contact-info {
    margin-bottom: 32px;
  }
  .contact-subtitle {
    color: #7c7c7c;
    font-size: 14px;
    font-weight: 500;
    line-height: 100%;
    text-transform: uppercase;
    margin-bottom: 4px;
  }
  .contact-title {
    color: #000;
    font-size: 20px;
    font-weight: 500;
    line-height: normal;
    text-transform: uppercase;
    margin-bottom: 24px;
  }
  .contact-btn-group > button {
    font-size: 16px;
    font-weight: 500;
    line-height: 100%;
    text-transform: uppercase;
    -webkit-flex-basis: 50%;
    flex-basis: 50%;
    height: 70px;
  }
  .contact-btn-group > button:first-child {
    margin-right: 8px;
  }
  .contact-btn-group > button:last-child {
    background-color: transparent;
    margin-left: 8px;
  }
  .contact-btn-group > button svg {
    margin-right: 10px;
  }
  #button-callback svg {
    fill: none;
    stroke: #fff;
  }
  #button-chat svg {
    fill: #000;
    stroke: none;
  }
  .contact-image {
    margin-bottom: 24px;
  }
  .work-ny {
    color: #000;
    /*font-size: 14px;*/
    font-weight: 500;
    margin-top: 3px;

  }
  @media (max-width: 1199px) {
    #information-contact h1 {
      font-size: 28px;
      margin: 0 0 24px;
    }
    #information-contact h4 {
      font-size: 28px;
      margin: 0 0 24px;
    }
    .contact-inner {
      padding: 24px;
      max-width: unset;
    }
    .contact-info {
      margin-bottom: 24px;
    }
    .contact-title {
      font-size: 18px;
      margin-bottom: 16px;
    }
  }
  @media (max-width: 991px) {
    #information-contact h1 {
      font-size: 24px;
    }
    #information-contact h4 {
      font-size: 24px;
    }
    .contact-inner {
      padding: 16px;
    }
  }
  @media (max-width: 767px) {
    #information-contact #content {
      padding: 0;
    }
    #information-contact h1 {
      font-size: 24px;
    }
    #information-contact h4 {
      font-size: 24px;
      margin: 0;
    }
    .contact-inner {
      padding: 24px 15px;
    }
    .contact-inner.open .hidden-xs {
      display: block!important;
    }
    .contact-inner.open .contact-info {
      margin-top: 24px;
    }
    .contact-inner .caret {
      display: block;
      float: right;
      border-top: 6px solid;
      border-right: 6px solid transparent;
      border-bottom: unset;
      border-left: 6px solid transparent;
    }
    .contact-inner.open .caret {
      border-top: unset;
      border-bottom: 6px solid #000;
      border-right: 6px solid transparent;
      border-left: 6px solid transparent;
    }
    .contact-btn-group {
      -webkit-flex-direction: column;
      flex-direction: column;
    }
    .contact-btn-group > button {
      font-size: 14px;
      -webkit-flex-basis: 100%;
      flex-basis: 100%;
      width: 100%;
      min-height: 60px;
      margin: 0!important;
    }
    .contact-btn-group > button:first-child {
      margin-bottom: 8px!important;
    }
    .contact-image {
      display: none;
    }
    .contact-map > iframe {
      height: 295px;
    }
  }
</style>
